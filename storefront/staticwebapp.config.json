{
  "routes": [
    {
      "route": "/",
      "rewrite": "/index.html"
    },
    {
      "route": "/blog",
      "rewrite": "/blog.html"
    },
    {
      "route": "/pricing",
      "rewrite": "/pricing.html"
    },
    {
      "route": "/404",
      "rewrite": "/404.html"
    },
    {
      "route": "/_astro/*",
      "headers": {
        "cache-control": "max-age=31536000"
      }
    },
    {
      "route": "/*",
      "rewrite": "/404.html"
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_astro/*", "/blog", "/pricing", "/404"]
  }
}
